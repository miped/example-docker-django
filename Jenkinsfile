    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "BUILD!"'
            }
        }
        stage('Test'){
            steps {
                sh 'echo "TEST MOAR!"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "DEPLOY!"'
            }
        }
    }
}
